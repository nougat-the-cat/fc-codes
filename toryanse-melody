melody = [
    ("E4", 600), ("E4", 300), ("D4", 300), 
    ("E4", 300), ("E4", 150), ("D4", 150), ("B3", 300), (" ", 300), 
    ("F4", 300), ("F4", 150), ("F4", 150), ("A4", 300), ("F4", 150), ("E4", 150), 
    ("F4", 150), ("E4", 150), ("D4", 150), ("D4", 150), ("E4", 150), (" ", 300) 
]

for note, duration in melody:
    freq = note_freqs[note]
    winsound.Beep(int(freq), duration*2)
